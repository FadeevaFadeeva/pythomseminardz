#dz1
n = int(input())
k = 0
for i in range(n):
    v = int(input())
    if v == 1:
        k += 1
print(k if k<n/2 else n-k)

#dz2

s, p = map(int, input().split())
x = (s-int((s**2-4*p)**0.5))//2
y = (s+int((s**2-4*p)**0.5))//2
print(x, y)

#dz3
N = int(input())
k = 0
while 2**k <= N:
    print(2**k, end=" ")
    k += 1
print()